{
  "expo": {
    "name": "Locatory",
    "slug": "locatory",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [],
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "package": "com.locatory.app"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "Locatory is a recommendation based mobile app that helps user finds details about places around Bangladesh",
    "extra": {
      "eas": {
        "projectId": "a3c945b5-ddd5-43b0-a9f2-48428843b6c6"
      }
    }
  }
}
